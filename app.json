{
  "expo": {
    "name": "PrintSwift",
    "slug": "PrintSwift",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
